{
    "name": "vscode-imageoptimizer",
    "displayName": "vscode-ImageOptimizer",
    "description": "A port of the Visual Studio extension ImageOptimizer for the VS Code editor.",
    "version": "0.0.1",
    "publisher": "tylerbhughes",
    "engines": {
        "vscode": "^1.21.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onCommand:extension.ImageOptimizer"
    ],
    "main": "./extension",
    "contributes": {
        "commands": [{
            "command": "extension.ImageOptimizer",
            "title": "Image Optimizer"
        }],
        "menus": {
            "explorer/context": [{
                "command": "extension.ImageOptimizer",
                "group": "7_modification"
          }]
        }
    },
    "scripts": {
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "typescript": "^2.6.1",
        "vscode": "^1.1.6",
        "eslint": "^4.11.0",
        "@types/node": "^7.0.43",
        "@types/mocha": "^2.2.42"
    }
}